.list-tit{ height:24px; width:100%; border-bottom:3px solid #dfdfdf; margin-top:15px; position:relative;}
.list-tit p{display: inline-block;
padding: 3px 13px;
border-bottom:3px solid #fd4e93;
font-size: 14px;
font-weight:bold;
line-height:19px;
color: #fd4e93; position:absolute; top:-5px; left:0px;font-family:Microsoft YaHei;
vertical-align: middle;  \0}
.list-tit p{ *top:-3px; }
.list-tit a { float:right; cursor:pointer; font-size:12px; color:#fd4e93; margin-top:7px;}
.icon  { display: inline-block }
.icon  { *display: inline;}
.icon { display:inline-block; width:20px; height:19px; background:url(../images/default/icon_01.jpg) no-repeat center center; position: relative;
top:4px;
left:0px;
}
.list-tit p span { +position:relative ; *+left:-4px; +top:1px;}
