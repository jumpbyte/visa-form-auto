@charset "utf-8";
/* CSS Document */

*{ padding:0; margin:0;
font-family: Arial,Helvetica,sans-serif;
font-size: 12px;
outline:none;} 
ul { list-style:none;}
a { text-decoration:none; color:#000;}
.clear{ clear:both;}
.font_gray{ color:#666;}
.main-wrap { padding:20px;  }
.header { 
overflow: hidden;
width: 100%;
min-width: 500px;}
.header .ifm{ text-align:center; color:#666;}
.page-l { width:48%; overflow:hidden;  float:left; }
.page-r {width:48%; float:left;  }
.new-meg{width:100%;}
.top-text h3 {text-align:center;}
.top-text h3 a{ font-size:14px; color:#fd4e93;  cursor:default; font-family:Microsoft YaHei; }
.top-text{ width:90%; float:left; font-size:12px; margin-left:2%}
.top-text p { margin-top:7px; height:50%;  line-height:24px;   overflow: hidden;  /*�������*/  text-overflow: ellipsis; /*��ʡ�Ժ�...��ʾ*/ max-height:120px; font-size:12px;}

 .top-img { width:42%; background:url(../images/default/img-01.jpg) no-repeat center center; height:158px; float:left; margin-top:3px; margin-left:1%;}
