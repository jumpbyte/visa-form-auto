<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>签证资料填写-其他信息</title>
    <script type="text/javascript" src="js/lib/se/reference.js"></script>    
    <script type="text/javascript" src="js/lib/se/visa_common_form.js"></script>   
    <script type="text/javascript">
        $().ready(function() {
            var options = {
                curPage:"O",
                pages:Pages,
                validateOptions: {
                    rules: {
                        email: {
                            required: true,
                            email: true
                        },
                        fistPhone: "phone",
                        reservePhone: "phone",
                        maillingAddress: {
                            required: true,
                            maxlength: 88
                        },
                        homeAddr: {
                            required: true,
                            maxlength: 88
                        }
                    },
                    messages: {
                        gender: "请选择性别！",
                        idCardCode: "请填写身份证号"
                    }
                }
            };
            var formHelper = new VisaFormHelper(options);
            formHelper.init();
            formHelper.bindEvents();
            formHelper.loadFormData();
            formHelper.bindFormData();
            visaCommon.attachChangeEvent();
            visaCommon.attachAddDelEvent();
	        if(usUtil.isWETInfoHidden(formHelper.data)){
                 $("#preBtn").data("pre-url","loadRelativeInfo");
                 formHelper.preUrl="loadRelativeInfo";
                 $("#preBtn").html("上一步:亲属信息");
                 $("#additionInfo").remove();
            }
 
            formHelper.getTabOp=function(page){
                return  usUtil.getTapOp(this.data,page);
            }
        });
    </script>
</head>

<body>
  <div class="main wrapper">
    <div  class="header">
        <div id="tab-nav" class="nav">
            <ul>
                <li class="active" data-tab-url="loadPersonalInfo" data-page="P">
                    <a href="javascript:void(0)" >个人信息</a>
                    <ul>
                        <li><a data-scroll-id="personInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#personInfoArea'),300)">基础信息</a></li>
                        <li><a data-scroll-id="addressPhoneInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#addressPhoneInfoArea'),300)">地址/电话信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadJourneyInfo"  data-page="T">
                    <a href="javascript:void(0)"  >旅程信息</a>
                    <ul>
                        <li><a data-scroll-id="passportInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#passportInfoArea'),300)">护照信息</a></li>
                        <li><a data-scroll-id="thisTripInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#thisTripInfo'),300)">本次旅程</a></li>
                        <li><a data-scroll-id="previousTripInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#previousTripInfo'),300)">曾经的旅程</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadRelativeInfo"  data-page="R">
                    <a href="javascript:void(0)">亲属信息</a>
                    <ul>
                        <li><a data-scroll-id="relationInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#relationInfoArea'),300)">亲属信息</a></li>
                        <li><a data-scroll-id="spouseArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#spouseArea'),300)">配偶信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadWorkEducationInfo"  data-page="E" >
                    <a href="javascript:void(0)">职业及教育经历</a>
                    <ul>
                        <li><a data-scroll-id="workInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#workInfo'),300)">职业信息</a></li>
                        <li><a data-scroll-id="eduInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#eduInfo'),300)">教育信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadOtherInfo" data-page="O" ><a href="javascript:void(0)" >补充信息</a></li>
            </ul>
        </div>
        <div class="step-indicator">
            <ol class="ui-step ui-step-6">
                <li class="step-start">
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">1</span>
                        <span class="ui-step-cont-text">填写个人信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">2</span>
                        <span class="ui-step-cont-text">填写旅程信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">3</span>
                        <span class="ui-step-cont-text">填写亲属信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">4</span>
                        <span class="ui-step-cont-text">填写职业与教育经历</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">5</span>
                        <span class="ui-step-cont-text">填写补充信息</span>
                    </div>
                </li>
                <li class="step-end">
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">5</span>
                        <span class="ui-step-cont-text">完成检查并提交</span>
                    </div>
                </li>
            </ol>
        </div>
    </div>
    <div class="table-form">
        <form id="visa-form" action="">
            <input id="para-country" type="hidden" name="para.country" value="us" />
            <input id="para-page-type" type="hidden" name="para.pageType" value="" />
            <input id="para-op" type="hidden" name="para.op" value="modify" />
            <input id="para-form-id" type="hidden" name="para.formId" value="" />
            <input id="para-form-num" type="hidden" name="para.formSerialNum" value="" />
            <input id="para-progress" type="hidden" name="para.progress" value="O" />
            <input id="para-lastStep" type="hidden" name="para.isLastStep" value="true" />
            <input id="para-cfgId" type="hidden" name="para.cfgId" value="0" />
        </form>
    </div>
    <div class="operate-menu-area">
        <div class="button float-left">
            <a id="preBtn" class="prestep" data-pre-url="loadWorkEducationInfo" href="javascript:void(0)">上一步:职业教育经历</a>
        </div>
        <div class="button float-left">
            <a id="saveBtn" class="save" href="javascript:void(0)">保存</a>
        </div>
        <div class="button float-left hidden">
            <a id="checkBtn" class="checkstep" href="javascript:void(0)">完成检查并提交</a>
        </div>
    </div>
    </div>
</body>
<script id="otherInfo-v-bind" type="text/html">
    <div id="otherInfo">
        <h2 class="area-title">补充信息</h2>
        {{if form.wetInfo!=null && form.wetInfo.addition!=null }}
        <div id="additionInfo">
            <div class="field-container">
                <label class="field-title">请提供一份您所说语言的清单</label>
                <input id="languages" name="languages" class="text-input" value="{{form.wetInfo.addition.languages?form.wetInfo.addition.languages:['']  | join}}" type="text" required />
            </div>
            <div class="field-container">
                <label class="field-title">您是否属于一个党派或宗族？</label>
                <input type="radio" id="belongClanOrTribeYes" name="belongClanOrTribe" {{form.wetInfo.addition.belongClanOrTribe ? 'checked="checked"':''}} value="Y" class="align-middle" />
                <label for="belongClanOrTribeYes" class="align-middle">是</label>
                <input type="radio" id="belongClanOrTribeNo" name="belongClanOrTribe" {{!form.wetInfo.addition.belongClanOrTribe ? 'checked="checked"':''}} value="N" class="align-middle" />
                <label for="belongClanOrTribeNo" class="align-middle">否</label>
                <div class="area info-area {{!form.wetInfo.addition.belongClanOrTribe?'hidden':''}} one">
                    <div class="field-container">
                        <span class="field-title">党派或宗族的名字</span>
                        <input id="clanOrTribeName" name="clanOrTribeName" class="text-input" value="{{form.wetInfo.addition.clanOrTribeName}}" type="text" required />
                    </div>
                </div>
            </div>
            <div class="field-container">
                <label class="field-title">您是否属于、捐助过或任职于任何职业机构、社会机构或慈善机构？</label>
                <input type="radio" id="contributedForOrganizationYes" name="contributedForOrganization" {{form.wetInfo.addition.contributedForOrganization ? 'checked="checked"':''}} value="Y" class="align-middle" />
                <label for="contributedForOrganizationYes" class="align-middle">是</label>
                <input type="radio" id="contributedForOrganizationNo" name="contributedForOrganization" {{!form.wetInfo.addition.contributedForOrganization ? 'checked="checked"':''}} value="N" class="align-middle" />
                <label for="contributedForOrganizationNo" class="align-middle">否</label>
                <div class="area info-area {{!form.wetInfo.addition.contributedForOrganization?'hidden':''}}  one">
                    <div class="field-container">
                        <span class="field-title">机构的名字</span>
                        <input id="organizationName" name="organizationName" value="{{form.wetInfo.addition.organizationName?form.wetInfo.addition.organizationName:[''] | join}}" class="text-input" type="text" required />
                    </div>
                </div>
            </div>
            <div class="field-container">
                <label class="field-title">您是否具有特殊技能或接受过特殊培训，例如有关枪械、炸药、核装置、生物或化学方面的经验？</label>
                <input type="radio" id="specializedSkillYes" name="specializedSkill" {{form.wetInfo.addition.specializedSkill ? 'checked="checked"':''}} value="Y" class="align-middle" />
                <label for="specializedSkillYes" class="align-middle">是</label>
                <input type="radio" id="specializedSkillNo" name="specializedSkill" {{!form.wetInfo.addition.specializedSkill ? 'checked="checked"':''}} value="N" class="align-middle" />
                <label for="specializedSkillNo" class="align-middle">否</label>
                <div class="area info-area {{!form.wetInfo.addition.specializedSkill?'hidden':''}} one">
                    <div class="field-container">
                        <span class="field-title">解释/原因说明</span>
                        <textarea required name="skillExplain" id="skillExplain" cols="31" rows="2">{{form.wetInfo.addition.skillExplain}}</textarea>
                    </div>
                </div>
            </div>
            <div class="field-container">
                <label class="field-title">您是否曾经在军队服役？</label>
                <input type="radio" id="servedYes" name="served" {{form.wetInfo.addition.served ? 'checked="checked"':''}} value="Y" data-tpl-id="served-template" class="align-middle" />
                <label for="servedYes" class="align-middle">是</label>
                <input type="radio" id="servedNo" name="served" {{!form.wetInfo.addition.served ? 'checked="checked"':''}} value="N" class="align-middle" />
                <label for="servedNo" class="align-middle">否</label>
                <div class="area info-area {{!form.wetInfo.addition.served?'hidden':''}} one">
                    <span class="field-title">请提供以下有关你曾服役的军队信息</span>
                    {{if form.wetInfo.addition.served}}
                    <input id="servedCount" type="hidden" name="servedCount" counter value="{{form.wetInfo.addition.militarys.length}}" />
                    {{each form.wetInfo.addition.militarys as n i}}
                    <div class="repeat">
                            <div class="field-container">
                                <span class="field-title">国家</span> {{#countries | select:{id:"servedCountry_"+i,name:"servedCountry_"+i,defaultValue:n.country,title:"请选择国家！",required:true} }}
                            </div>
                            <div class="field-container">
                                <label class="field-title">军种</label>
                                <input id="branch_{{i}}" name="branch_{{i}}" class="text-input" type="text" value="{{n.branch}}" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">级别或职位</label>
                                <input id="rank_{{i}}" name="rank_{{i}}" class="text-input" type="text" value="{{n.rank}}" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">军事特长</label>
                                <input id="specialtyv_{{i}}" name="specialty_{{i}}" class="text-input" type="text" value="{{n.specialty}}" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">服役开始日期</label>
                                <input id="servedFromDate_{{i}}" name="servedFromDate_{{i}}" class="Wdate text-input" value="{{n.from}}" type="text" required onclick="WdatePicker()" />
                            </div>
                            <div class="field-container">
                                <label class="field-title">服役结束日期</label>
                                <input id="servedToDate_{{i}}" name="servedToDate_{{i}}" class="Wdate text-input" value="{{n.to}}" type="text" required onclick="WdatePicker()" />
                            </div>
                    </div>
                    {{/each}}
                    {{else}}
                    <input type="hidden" id="servedCount" name="servedCount" counter value="0" />
                    {{/if}}

                    <!--服役军队占位-->
                    <div id="operate_served_menu" class="control-menu">
                        <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                        <a href="javascript:void(0)">删除</a>
                    </div>
                </div>
            </div>
            <div class="field-container">
                <label class="field-title">您是否曾经服务于或参与过准军事单位、治安团体、造反组织、游击队或暴动组织，或曾经是其中一员？</label>
                <input type="radio" id="servedForRebelOrgYes" name="servedForRebelOrg" {{form.wetInfo.addition.servedForRebelOrg ? 'checked="checked"':''}} value="Y" class="align-middle" />
                <label for="isOneOfTerroristYes" class="align-middle">是</label>
                <input type="radio" id="servedForRebelOrgNo" name="servedForRebelOrg" {{!form.wetInfo.addition.servedForRebelOrg ? 'checked="checked"':''}} value="N" class="align-middle" />
                <label for="isOneOfTerroristNo" class="align-middle">否</label>
                <div class="area info-area {{!form.wetInfo.addition.servedForRebelOrg?'hidden':''}} one">
                    <div class="field-container">
                        <span class="field-title">解释/原因说明</span>
                        <textarea required name="rebelExplain" id="rebelExplain" cols="31" rows="2">{{form.wetInfo.addition.rebelExplain}}</textarea>
                    </div>
                </div>
            </div>
        </div>
        {{/if}}
        <div class="field-container">
            <label class="field-title">您是否患有涉及公共卫生的传染病？（按照美国卫生和公众服务部界定，涉及公共卫生的传染病包括软下疳，淋病，腹股沟肉芽肿，传染性麻风，性病性淋巴肉芽肿，感染期梅毒、软活动性肺结核等）</label>
            <input type="radio" id="haveDiseaseYes" name="haveDisease" value="Y" {{form.backgroundInfo.haveDisease ? 'checked="checked"':''}} class="align-middle" />
            <label for="haveDiseaseYes" class="align-middle">是</label>
            <input type="radio" id="haveDiseaseNo" name="haveDisease" value="N" {{!form.backgroundInfo.haveDisease ? 'checked="checked"':''}} class="align-middle" />
            <label for="haveDiseaseNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.haveDisease?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="diseaseExplain" id="diseaseExplain" cols="31" rows="2">{{form.backgroundInfo.diseaseExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是否患有对其他人的人身安全及利益造成威胁的精神或身体疾病？</label>
            <input type="radio" id="haveMentalYes" name="haveMental" value="Y" {{form.backgroundInfo.haveMental ? 'checked="checked"':''}} class="align-middle" />
            <label for="haveMentalYes" class="align-middle">是</label>
            <input type="radio" id="haveMentalNo" name="haveMental" value="N" {{!form.backgroundInfo.haveMental ? 'checked="checked"':''}} class="align-middle" />
            <label for="haveMentalNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.haveMental?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="mentalExplain" id="mentalExplain" cols="31" rows="2">{{form.backgroundInfo.mentalExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是否曾经滥用药物并上瘾？</label>
            <input type="radio" id="dragAddictYes" name="dragAddict" value="Y" {{form.backgroundInfo.dragAddict ? 'checked="checked"':''}} class="align-middle" />
            <label for="dragAddictYes" class="align-middle">是</label>
            <input type="radio" id="dragAddictNo" name="dragAddict" value="N" {{!form.backgroundInfo.dragAddict ? 'checked="checked"':''}} class="align-middle" />
            <label for="dragAddictNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.dragAddict?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="dragExplain" id="dragExplain" cols="31" rows="2">{{form.backgroundInfo.dragExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是否曾经因违法或犯罪被捕或被判罪，即使是后来获得了宽恕、赦免或其它类似的裁决?</label>
            <input type="radio" id="arrestedBeforeYes" name="arrestedBefore" value="Y" {{form.backgroundInfo.arrestedBefore ? 'checked="checked"':''}} class="align-middle" />
            <label for="arrestedBeforeYes" class="align-middle">是</label>
            <input type="radio" id="arrestedBeforeNo" name="arrestedBefore" value="N" {{!form.backgroundInfo.arrestedBefore ? 'checked="checked"':''}} class="align-middle" />
            <label for="arrestedBeforeNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.arrestedBefore?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="arrestedExplain" id="arrestedExplain" cols="31" rows="2">{{form.backgroundInfo.arrestedExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是否曾经违反或密谋违反有关管控物资方面的法律？</label>
            <input type="radio" id="violatedLawYes" name="violatedLaw" value="Y" {{form.backgroundInfo.violatedLaw ? 'checked="checked"':''}} class="align-middle" />
            <label for="violatedLawYes" class="align-middle">是</label>
            <input type="radio" id="violatedLawNo" name="violatedLaw" value="N" {{!form.backgroundInfo.violatedLaw ? 'checked="checked"':''}} class="align-middle" />
            <label for="violatedLawNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.violatedLaw?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="lawExplain" id="lawExplain" cols="31" rows="2"><{{form.backgroundInfo.lawExplain}}/textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是来美国从事卖淫或非法商业性交易吗？在过去的十年中，您是否从事过卖淫或组织介绍过卖淫？</label>
            <input type="radio" id="engagedUnlawfulThingYes" name="engagedUnlawfulThing" value="Y" {{form.backgroundInfo.engagedUnlawfulThing ? 'checked="checked"':''}} class="align-middle" />
            <label for="engagedUnlawfulThingYes" class="align-middle">是</label>
            <input type="radio" id="engagedUnlawfulThingNo" name="engagedUnlawfulThing" value="N" {{!form.backgroundInfo.engagedUnlawfulThing ? 'checked="checked"':''}} class="align-middle" />
            <label for="engagedUnlawfulThingNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.engagedUnlawfulThing?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="unlawfulThingExplain" id="unlawfulThingExplain" cols="31" rows="2">{{form.backgroundInfo.unlawfulThingExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您是否曾经参与或意图从事洗钱活动？</label>
            <input type="radio" id="involvedInlaunderingYes" name="involvedInlaundering" value="Y" {{form.backgroundInfo.involvedInlaundering ? 'checked="checked"':''}} class="align-middle" />
            <label for="involvedInlaunderingYes" class="align-middle">是</label>
            <input type="radio" id="involvedInlaunderingNo" name="involvedInlaundering" value="N" {{!form.backgroundInfo.involvedInlaundering ? 'checked="checked"':''}} class="align-middle" />
            <label for="involvedInlaunderingNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.involvedInlaundering?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="inlaunderingExplain" id="inlaunderingExplain" cols="31" rows="2">{{form.backgroundInfo.inlaunderingExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <label class="field-title">您曾在美国或美国以外的地方犯有或密谋人口走私罪吗？</label>
            <input type="radio" id="conspiredHumanTraffickingYes" name="conspiredHumanTrafficking" value="Y" {{form.backgroundInfo.conspiredHumanTrafficking ? 'checked="checked"':''}} class="align-middle" />
            <label for="conspiredHumanTraffickingYes" class="align-middle">是</label>
            <input type="radio" id="conspiredHumanTraffickingNo" name="conspiredHumanTrafficking" value="N" {{!form.backgroundInfo.conspiredHumanTrafficking ? 'checked="checked"':''}} class="align-middle" />
            <label for="conspiredHumanTraffickingNo" class="align-middle">否</label>
            <div class="area info-area {{!form.backgroundInfo.conspiredHumanTrafficking?'hidden':''}} one">
                <div class="field-container">
                    <span class="field-title">解释/原因说明</span>
                    <textarea required name="traffickingExplain" id="traffickingExplain" cols="31" rows="2">{{form.backgroundInfo.traffickingExplain}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
