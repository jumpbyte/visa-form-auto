<script id="served-template" type="text/html">
    <div class="repeat">
        <div class="field-container">
            <span class="field-title">国家</span> {{#countries | select:{id:"servedCountry",name:"servedCountry",defaultValue:"CHIN",title:"请选择国家！",required:true} }}
        </div>
        <div class="field-container">
            <label class="field-title">军种</label>
            <input id="branch" name="branch" class="text-input" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">级别或职位</label>
            <input id="rank" name="rank" class="text-input" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">军事特长</label>
            <input id="specialty" name="specialty" class="text-input" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">服役开始日期</label>
            <input id="servedFromDate" name="servedFromDate" class="Wdate text-input" type="text" required onclick="WdatePicker()" />
        </div>
        <div class="field-container">
            <label class="field-title">服役结束日期</label>
            <input id="servedToDate" name="servedToDate" class="Wdate text-input" type="text" required onclick="WdatePicker()" />
        </div>
    </div>
</script>

