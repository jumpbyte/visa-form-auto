<!DOCTYPE html PUBLIC"-//W3C//DTD HTML 4.01 Transitional//EN""http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>签证资料填写-基本信息</title>
    <link href="css/common.css" rel="stylesheet" type="text/css" />
    <link href="css/lib/se/vsia_common_form.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="js/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="js/common/template.js"></script>
    <script type="text/javascript" src="js/common/common.js"></script>
    <script type="text/javascript" src="plugin/My97DatePicker/WdatePicker.js"></script>
    <script type="text/javascript" src="plugin/jquery-validation/jquery.validate.js"></script>
    <script type="text/javascript" src="plugin/jquery-validation/localization/messages_zh.js"></script>
    <script type="text/javascript" src="js/lib/se/visa_us_data_json.js"></script>
    <script type="text/javascript" src="js/lib/se/visa_common_form.js"></script>

    <script type="text/javascript">
        $().ready(function() {
            var options = {
                validateOptions: {
                    rules: {
                        email: {
                            required: true,
                            email: true
                        },
                        fistPhone: "phone",
                        reservePhone: "phone",
                        maillingAddress: {
                            required: true,
                            maxlength: 88
                        },
                        homeAddr: {
                            required: true,
                            maxlength: 88
                        }
                    },
                    messages: {
                        gender: "请选择性别！",
                        idCardCode: "请填写身份证号"
                    }
                }
            };
            var formHelper = new VisaFormHelper(options);
            formHelper.init();
            formHelper.bindEvents();
        });
    </script>
</head>

<body>
    <div class="table-form">
        <form id="visa-form" action="collect">
            <input id="para-country" type="hidden" name="para.country" value="us" />
            <input id="para-page-type" type="hidden" name="para.pageType" value="" />
            <input id="para-op" type="hidden" name="para.op" value="add" />
            <input id="para-form-id" type="hidden" name="para.formId" value="" />
            <input id="para-form-num" type="hidden" name="para.formSerialNum" value="" />
            <input id="para-progress" type="hidden" name="para.progress" value="P" />
            <!--基础信息-->
            <div id="personInfoArea">
                <h2 class="area-title">基础信息</h2>
                <div class="field-container">
                    <label for="surnName" class="field-title">姓</label>
                    <input type="text" required id="surnName" name="surnName" class="text-input" />
                </div>
                <div class="field-container">
                    <label for="givenName" class="field-title">名</label>
                    <input type="text" required id="givenName" name="givenName" class="text-input" />
                </div>
                <div class="field-container">
                    <label class="field-title">是否有曾用名？</label>
                    <input type="radio" id="usedNameYes" name="isHaveUsedName" value="Y" data-tpl-id="usedname-template" class="align-middle" />
                    <label for="usedNameYes" class="align-middle">是</label>
                    <input type="radio" id="usedNameNo" name="isHaveUsedName" value="N" checked="checked" class="align-middle" />
                    <label for="usedNameNo" class="align-middle">否</label>
                    <div class="area info-area hidden one">
                        <label style="display: block">请填写如下信息</label>
                        <input type="hidden" id="usedNameCount" counter name="usedNameCount" value="0" />
                        <hr/>
                        <!--占位-->
                        <div id="operate_usedName_menu" class="control-menu">
                            <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                            <a href="javascript:void(0)">删除</a>
                        </div>
                    </div>
                </div>
                <div class="field-container">
                    <label for="telecodeSurnName" class="field-title">姓的电报码</label>
                    <input type="text" required id="telecodeSurnName" name="telecodeSurnName" class="text-input" />
                </div>
                <div class="field-container">
                    <label for="telecodeGivenName" class="field-title">名的电报码</label>
                    <input type="text" required id="telecodeGivenName" name="telecodeGivenName" class="text-input" />
                </div>
                <div class="field-container">
                    <label class="field-title">性别</label>
                    <input type="radio" id="genderM" name="gender" value="1" required class="align-middle" />
                    <label for="genderM" class="align-middle">男</label>
                    <input type="radio" id="genderF" name="gender" value="0" class="align-middle" />
                    <label for="genderF" class="align-middle">女</label>
                    <label for="gender" class="error">请选择性别！</label>
                </div>
                <div class="field-container">
                    <label class="field-title">婚姻状况</label>
                    <select id="maritalStatus" title="必须选一项婚姻状况！" name="maritalStatus" required>
                        <option value >--请选择--</option>
                        <option value="M">已婚</option>
                        <option value="C">普通法律婚姻</option>
                        <option value="P">民事婚姻/国内同居</option>
                        <option value="S">单身</option>
                        <option value="W">丧偶</option>
                        <option value="D">离异</option>
                        <option value="L">合法分居</option>
                        <option value="O">其他</option>
                    </select>
                </div>
                <div class="field-container">
                    <label class="field-title">出生日期</label>
                    <input id="birthday" name="birthday" class="Wdate text-input" type="text" required onclick="WdatePicker()" />
                </div>
                <div class="field-container">
                    <label for="birthCity" class="field-title">出生城市</label>
                    <input type="text" required id="birthCity" name="birthCity" class="text-input">
                </div>
                <div class="field-container">
                    <label for="birthProvince" class="field-title">出生省份</label>
                    <input type="text" required id="birthProvince" name="birthProvince" class="text-input">
                </div>
                <div class="field-container">
                    <label for="birthCountry" class="field-title">出生国家</label>
                    <select id="birthCountry" name="birthCountry">
                    <option value="CHIN" selected="selected">中国</option>
                    <option value="HNK">中国香港特别行政区</option>
                    <option value="HOKO">香港BNO</option>
				</select>
                </div>
                <div class="field-container">
                    <label for="nationality" class="field-title">国籍</label>
                    <select id="nationality" name="nationality">
						<option value="CHIN" selected="selected">中国</option>
				</select>
                </div>
                <div class="field-container">
                    <label class="field-title">除了上述国籍外，您是否还拥有或曾经拥有其他国籍？</label>
                    <input type="radio" id="isHaveOtherNationalityYes" name="isHaveOtherNationality" value="Y" data-tpl-id="other-nationality-template" class="align-middle" />
                    <label for="isHaveOtherNationalityYes" class="align-middle">是</label>
                    <input type="radio" id="isHaveOtherNationalityNo" name="isHaveOtherNationality" value="N" checked="checked" class="align-middle" />
                    <label for="isHaveOtherNationalityNo" class="align-middle">否</label>
                    <div class="area info-area one hidden">
                        <label style="display: block">请列举拥有的国籍</label>
                        <hr/>
                        <input type="hidden" id="otherNationalityCount" counter name="otherNationalityCount" value="0" />
                        <!--占位-->
                        <div id="operate_otherNationality_menu" class="control-menu">
                            <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                            <a href="javascript:void(0)">删除</a>
                        </div>
                    </div>
                    <br>
                </div>
                <!-- 永久居住 -->
                <div class="field-container">
                    <label class="field-title">您现在是其他国家/地区的永久居民吗？</label>
                    <input type="radio" id="isOtherPermanentYes" name="isOtherPermanent" value="Y" data-tpl-id="permanentCountry-template" class="align-middle" />
                    <label for="isOtherPermanentYes" class="align-middle">是</label>
                    <input type="radio" id="isOtherPermanentNo" name="isOtherPermanent" value="N" checked="checked" class="align-middle" />
                    <label for="isOtherPermanentNo" class="align-middle">否</label>
                    <div class="area info-area one hidden">
                        <label style="display: block">请列举永久居民的国家</label>
                        <hr/>
                        <input type="hidden" id="permanentCountryCount" name="permanentCountryCount" counter value="0" />
                        <!--永久居民国家占位-->
                        <div id="operate_permanent_menu" class="control-menu">
                            <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                            <a href="javascript:void(0)">删除</a>
                        </div>
                    </div>
                </div>
                <div class="field-container">
                    <label class="field-title">身份证号</label>
                    <input type="text" id="idCardCode" name="idCardCode" required class="text-input" />
                    <input type="checkbox" id="notIdCardNum" name="notIdCardNum" value="Y" />
                    <label for="notIdCardNum">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="securityNumber" class="field-title">美国社会安全号码</label>
                    <input type="text" required id="securityNumber" name="securityNumber" class="text-input" disabled/>
                    <input type="checkbox" id="securityNumberBox" name="securityNumberBox" value="Y" checked="checked" />
                    <label for="securityNumberBox">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="securityNumber" class="field-title">美国纳税人身份号码</label>
                    <input type="text" required id="taxpayerNumber" name="taxpayerNumber" class="text-input" disabled/>
                    <input type="checkbox" id="taxpayerNumberBox" name="taxpayerNumberBox" value="Y" checked="checked" />
                    <label for="taxpayerNumberBox">没有/无法提供</label>
                </div>
            </div>
            <div id="addressPhoneInfoArea">
                <h2 class="area-title">电话/地址信息</h2>
                <div class=" field-container">
                    <label for="homeAddr" class="field-title">家庭地址（英文）</label>
                    <input type="text" required id="homeAddr" name="homeAddr" class="text-input" />
                    <span style="color: red;display: block">备注:请录入英文地址，限88个字符（包含字母和标点符号）</span>
                </div>
                <div class="field-container">
                    <label for="homeCity" class="field-title">城市</label>
                    <input type="text" required id="homeCity" name="homeCity" class="text-input">
                </div>
                <div class="field-container">
                    <label for="homeProvince" class="field-title">省份</label>
                    <input type="text" required id="homeProvince" name="homeProvince" class="text-input">
                    <input type="checkbox" id="haveHomeProvinceBox" name="haveHomeProvinceBox" value="Y" />
                    <label for="haveHomeProvinceBox">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="homePostCode" class="field-title">邮编</label>
                    <input type="text" required id="homePostCode" name="homePostCode" class="text-input">
                    <input type="checkbox" id="haveHomePostCodeBox" name="haveHomePostCodeBox" value="Y" />
                    <label for="haveHomePostCodeBox">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="homeCountry" class="field-title">国家</label>
                    <select id="homeCountry" name="homeCountry">
						<option value="CHIN" selected="selected">中国</option>
					</select>
                </div>
                <div class="field-container">
                    <label class="field-title">通信地址是否与家庭住址相同？</label>
                    <input type="radio" id="sameAddressYes" name="isSameAddress" value="Y" data-is-show="N" checked="checked" class="align-middle" />
                    <label for="sameAddressYes" class="align-middle">是</label>
                    <input type="radio" id="sameAddressNo" name="isSameAddress" value="N" data-is-show="Y" class="align-middle" />
                    <label for="sameAddressNo" class="align-middle">否</label>
                    <div class="area info-area one hidden">
                        <label style="display: block">请填写如下信息</label>
                        <hr/>
                        <div class="field-container">
                            <label for="maillingAddress" class="field-title">通信地址</label>
                            <input type="text" required id="maillingAddress" name="maillingAddress" class="text-input">
                        </div>
                        <div class="field-container">
                            <label for="maillingCity" class="field-title">城市</label>
                            <input type="text" required id="maillingCity" name="maillingCity" class="text-input">
                        </div>
                        <div class="field-container">
                            <label for="maillingProvince" class="field-title">省份</label>
                            <input type="text" required id="maillingProvince" name="maillingProvince" class="text-input" />
                            <input type="checkbox" id="notHaveMaillingProvince" name="notHaveMaillingProvince" value="Y" />
                            <label for="notHaveMaillingProvince">没有/无法提供</label>
                        </div>
                        <div class="field-container">
                            <label class="field-title">邮编</label>
                            <input type="text" required id="maillingPostCode" name="maillingPostCode" class="text-input" />
                            <input type="checkbox" id="notHaveMaillingPostCode" name="notHaveMaillingPostCode" value="Y" />
                            <label for="notHaveMaillingPostCode">没有/无法提供</label>
                        </div>
                        <div class="field-container">
                            <label for="maillingCountry" class="field-title">国家</label>
                            <select id="maillingCountry" title="请选择国家" name="maillingCountry" required>
                            <option value>--请选择--</option>
                            <option value="CHIN" selected="selected" >中国</option>
                        </select>
                        </div>
                    </div>
                </div>
                <div class="field-container">
                    <label for="firstPhone" class="field-title">首选电话号码</label>
                    <input type="text" required id="firstPhone" name="firstPhone" class="text-input" />
                    <input type="checkbox" id="notHaveFirstPhone" name="notHaveFirstPhone" value="Y" />
                    <label for="notHaveFirstPhone">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="reservePhone" class="field-title">备选电话号码</label>
                    <input type="text" required id="reservePhone" name="reservePhone" class="text-input" />
                    <input type="checkbox" id="notHaveReservePhone" name="notHaveReservePhone" value="Y" />
                    <label for="notHaveReservePhone">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label class="field-title">电子邮箱</label>
                    <input type="email" required id="email" name="email" class="text-input" />
                </div>
            </div>
        </form>
    </div>
    <div class="operate-menu-area">
        <div class="button float-left">
            <a id="saveBtn" class="save" href="javascript:void(0)">保存</a>
        </div>
        <div class="button float-left">
            <a id="nextBtn" class="nextstep" data-next-url="loadJourneyInfo" href="javascript:void(0)">下一步:旅程信息</a>
        </div>
    </div>
</body>
<script id="usedname-template" type="text/html">
    <div class="repeat">
        <div class="field-container">
            <label class="field-title">曾用姓</label>
            <input id="usedSurnName" name="usedSurnName" class="text-input" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">曾用名</label>
            <input id="usedGivenName" name="usedGivenName" class="text-input" type="text" required />
        </div>
    </div>
</script>
<script id="other-nationality-template" type="text/html">
    <div class="repeat">
        <div class="field-container">
            <label for="otherNationality" class="field-title">其他国籍</label>
            <select id="otherNationality" title="请选择国籍！" name="otherNationality" required>
                <option value>--请选择--</option>
                <option value="CHIN">中国</option>
            </select>
        </div>
        <div class="field-container">
            <label class="field-title">您是否拥此国家或地区（国籍）的护照？</label>
            <input type="radio" id="isHaveOtherPassport" name="isHaveOtherPassport" value="Y" class="align-middle" />
            <label for="otherPassportYes" class="align-middle">是</label>
            <input type="radio" id="isHaveOtherPassport" name="isHaveOtherPassport" value="N" class="align-middle" />
            <label for="otherPassportNo" class="align-middle">否</label>
            <div class="area info-area two hidden">
                <div class="field-container">
                    <label for="otherPassportNum" class="field-title">护照号</label>
                    <input id="otherPassportNum" name="otherPassportNum" class="text-input" type="text" required />
                </div>
            </div>
        </div>
    </div>
</script>

<script id="permanentCountry-template" type="text/html">
    <div class="repeat">
        <div class="field-container">
            <label for="permanent" class="field-title">国家</label>
            <select id="permanentCountry" title="请选择永久居住国家！" name="permanentCountry" required>
                <option value>--请选择--</option>
                <option value="CHIN">中国</option>
            </select>
        </div>
    </div>
</script>

<script id="select_template" type="text/html">
    <select id="{{id}}" name="{{name}}" title="{{title}}" {{if required}} required {{/if}}>
        <option value>---请选择--</option>
        {{each list as op index}}
            {{if  defaultValue==op.value }} 
               <option value="{{op.value}}" selected="selected">{{op.text}}</option>
               {{else}}
               <option value="{{op.value}}">{{op.text}}</option>
            {{/if}}
        {{/each}}
    </select>
</script>

</html>