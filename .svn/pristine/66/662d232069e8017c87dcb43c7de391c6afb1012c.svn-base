ul,li { margin:0; padding:0; }
.li_left { float:left; }

.tab { width:100%; height:auto; overflow:hidden; margin-top:10px; }
.tab-title { width:100%; height:24px; border-bottom:1px solid #CCC; margin-bottom:5px;  }
.tab-title ul li { height:24px; float:left; font-size:12px; }

.tab-nav	{ width:100%; margin-left:10px; border:0px; height:24px; line-height:24px; margin-bottom:5px; }
.tab-nav ul li	{ float:left; margin:0 1px; text-align:center; }
.tab-nav li	{ width:80px; float:left; margin:0 1px; }

.tab-nav-step { background:#848484; color:#FFFFFF; cursor:pointer; }
.tab-nav-slted	{ background:#f03083; color:#FFFFFF; cursor:text; }

.tab-box	{ width:100%; margin:0 auto; }
.tab-box li { width:100%; height:auto; padding:0; float:left; overflow:hidden; }
.tab-box-data { margin:0 auto; padding:0 auto;}

.tab-bottom { width:100%; margin:0 auto; }
.tab-bottom li { width:100%; height:auto; padding:0; float:right; overflow:hidden; }

#boxService ul li{ width:20px; float:left; height:20px; line-height:20px; padding:6px 10px; }