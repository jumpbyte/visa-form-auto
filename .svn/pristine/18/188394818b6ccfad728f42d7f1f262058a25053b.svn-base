ul,li { margin:0; padding:0; }
.li_left { float:left; }

.tab { width:100%; height:auto; overflow:hidden; margin-top:10px; }
.tab-title { width:100%; height:66px; border-bottom:2px solid #999999; margin-bottom:10px;  }
.tab-title ul li { height:48px; float:left; font-size:12.5px; }

.tab-nav	{ width:100%; margin-left:10px; border:0px; height:48px; line-height:48px; margin-bottom:10px; }
.tab-nav ul li	{ float:left; margin:0 1px;}
.tab-nav li	{ width:220px; float:left; margin:0 1px; border:1px solid #cccccc; }
.tab-nav-index { display:block; float:left; padding:8px; font:bold 24px Verdana, Arial, Helvetica, sans-serif; }

.tab-nav-step { background:#f1f1f1; color:#999999; cursor:text;}
.tab-nav-Lastslted	{ background:#8cc63e; color:#ffffff; cursor:pointer; }
.tab-nav-slted	{ background:#ea8511; color:#FFFFFF; }

.tab-box	{ width:100%; margin:0 auto; }
.tab-box li { width:100%; height:auto; padding:0; float:left; overflow:hidden; }
.tab-box-data { margin:0 auto; padding:0 auto;}

.tab-bottom { width:100%; margin:0 auto; }
.tab-bottom li { width:100%; height:auto; padding:0; float:right; overflow:hidden; }
#boxService ul li{width:80px; float:left; height:20px; line-height:20px;}