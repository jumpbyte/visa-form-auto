<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>签证资料填写-职业与教育信息</title>
    <script type="text/javascript" src="js/lib/se/reference.js"></script>    
    <script type="text/javascript" src="js/lib/se/visa_common_form.js"></script>   
    <script type="text/javascript">
        $().ready(function () {

            var options = {
                curPage:"E",
                pages:Pages,
                validateOptions: {
                    rules: {
                        email: {
                            required: true,
                            email: true
                        },
                        unitStreet: {
                            required: true,
                            maxlength: 88
                        }
                    },
                    messages: {
                        gender: "请选择性别！",
                        idCardCode: "请填写身份证号"
                    }
                }
            };
            var formHelper = new VisaFormHelper(options);
            formHelper.getNextOp=function(){
                if(formHelper.data){
                     return formHelper.data.backgroundInfo ?Constants.UPDATE:Constants.ADD;
                }else{
                    return Constants.ADD
                }
            }
            formHelper.getTabOp=function(page){
                return  usUtil.getTapOp(this.data,page);
            }
            formHelper.init();
            formHelper.bindEvents();
            formHelper.loadFormData();
            formHelper.bindFormData();
            visaCommon.attachChangeEvent();
            visaCommon.attachAddDelEvent();
            initOccupation();
        });
        function  initOccupation(){
            $("#primaryOccupation").attr("data-show-condition", "");
            $("#primaryOccupation").children("option[value='N']").data("show-area", "noWorkArea")
            $("#primaryOccupation").children("option[value='O']").data("show-area", "#otherWorkArea,#workArea")
            $("#primaryOccupation").children("option[value!='O'][value!='N'][value!='RT'][value!='H'][value!='']").data("show-area", 'workArea');
        }
    </script>
</head>

<body>
 <div class="main wrapper">
    <div  class="header">
        <div id="tab-nav" class="nav">
            <ul>
                <li class="active" data-tab-url="loadPersonalInfo" data-page="P">
                    <a href="javascript:void(0)" >个人信息</a>
                    <ul>
                        <li><a data-scroll-id="personInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#personInfoArea'),300)">基础信息</a></li>
                        <li><a data-scroll-id="addressPhoneInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#addressPhoneInfoArea'),300)">地址/电话信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadJourneyInfo"  data-page="T">
                    <a href="javascript:void(0)"  >旅程信息</a>
                    <ul>
                        <li><a data-scroll-id="passportInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#passportInfoArea'),300)">护照信息</a></li>
                        <li><a data-scroll-id="thisTripInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#thisTripInfo'),300)">本次旅程</a></li>
                        <li><a data-scroll-id="previousTripInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#previousTripInfo'),300)">曾经的旅程</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadRelativeInfo"  data-page="R">
                    <a href="javascript:void(0)">亲属信息</a>
                    <ul>
                        <li><a data-scroll-id="relationInfoArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#relationInfoArea'),300)">亲属信息</a></li>
                        <li><a data-scroll-id="spouseArea" href="javascript:void(0)" onclick="$(window).scrollTo($('#spouseArea'),300)">配偶信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadWorkEducationInfo"  data-page="E" >
                    <a href="javascript:void(0)">职业及教育经历</a>
                    <ul>
                        <li><a data-scroll-id="workInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#workInfo'),300)">职业信息</a></li>
                        <li><a data-scroll-id="eduInfo" href="javascript:void(0)" onclick="$(window).scrollTo($('#eduInfo'),300)">教育信息</a></li>
                    </ul>
                </li>
                <li data-tab-url="loadOtherInfo" data-page="O" ><a href="javascript:void(0)" >补充信息</a></li>
            </ul>
        </div>
        <div class="step-indicator">
            <ol class="ui-step ui-step-6">
                <li class="step-start">
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">1</span>
                        <span class="ui-step-cont-text">填写个人信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">2</span>
                        <span class="ui-step-cont-text">填写旅程信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">3</span>
                        <span class="ui-step-cont-text">填写亲属信息</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">4</span>
                        <span class="ui-step-cont-text">填写职业与教育经历</span>
                    </div>
                </li>
                <li>
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">5</span>
                        <span class="ui-step-cont-text">填写补充信息</span>
                    </div>
                </li>
                <li class="step-end">
                    <div class="ui-step-line"></div>
                    <div class="ui-step-cont">
                        <span class="ui-step-cont-number">5</span>
                        <span class="ui-step-cont-text">完成检查并提交</span>
                    </div>
                </li>
            </ol>
        </div>
    </div>
    <div class="table-form">
        <form id="visa-form" action="">
            <input id="para-country" type="hidden" name="para.country" value="us" />
            <input id="para-page-type" type="hidden" name="para.pageType" value="" />
            <input id="para-op" type="hidden" name="para.op" value="modify" />
            <input id="para-form-id" type="hidden" name="para.formId" value="" />
            <input id="para-form-num" type="hidden" name="para.formSerialNum" value="" />
            <input id="para-progress" type="hidden" name="para.progress" value="E" />
            <input id="para-cfgId" type="hidden" name="para.cfgId" value="0" />
        </form>
    </div>
    <div class="operate-menu-area">
        <div class="button float-left">
            <a id="preBtn" class="prestep" data-pre-url="loadRelativeInfo" href="javascript:void(0)">上一步:亲属信息</a>
        </div>
        <div class="button float-left">
            <a id="saveBtn" class="save" href="javascript:void(0)">保存</a>
        </div>
        <div class="button float-left">
            <a id="nextBtn" class="nextstep" data-next-url="loadOtherInfo" href="javascript:void(0)">下一步:其他信息</a>
        </div>
    </div>
 </div>
</body>


<script id="workInfo-v-bind" type="text/html">
    <div id="workInfo">
        <h2 class="area-title">职业信息</h2>
        <div class="field-container">
            <span class="field-title">现在的职业</span>
            {{#occupations | select:{id:"primaryOccupation",name:"primaryOccupation",defaultValue:form.wetInfo.primaryOccupation,title:"请选择职业！",required:true} }}
            <div id="noWorkArea" class="area info-area one {{form.wetInfo.primaryOccupation!='N' &&'hidden'}}">
                <div class="field-container">
                    <label for="noWorkExplain" class="field-title">待业原因说明</label>
                    <textarea required name="noWorkExplain" id="noWorkExplain" cols="31" rows="2">{{form.wetInfo.explain}}</textarea>
                </div>
            </div>
            <div id="otherWorkArea" class="area info-area one {{form.wetInfo.primaryOccupation!='O' &&'hidden'}}">
                <div class="field-container">
                    <label for="otherOccupation" class="field-title">其他职业说明</label>
                    <textarea required name="otherOccupation" id="otherOccupation" cols="31" rows="2">{{form.wetInfo.other}}</textarea>
                </div>
            </div>
            <div id="workArea" class="area {{(form.wetInfo.primaryOccupation=='N'||form.wetInfo.primaryOccupation=='RT'||form.wetInfo.primaryOccupation=='H') &&'hidden'}}">
                <div class="field-container">
                    <label class="field-title">现在工作单位或学校名称</label>
                    <input id="unitName" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.name}}" name="unitName" class="text-input english" type="text" required />
                </div>
                <div class="field-container">
                    <label class="field-title">现在工作单位或学校地址</label>
                    <input id="unitStreet" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.address.street}}" name="unitStreet" class="address-input address" type="text" required />
                </div>
                <div class="field-container">
                    <label class="field-title">城市</label>
                    <input id="unitCity" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.address.city}}" name="unitCity" class="text-input" type="text" required />
                </div>
                <div class="field-container">
                    <label class="field-title">省份</label>
                    <input id="unitProvince" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.address.province}}" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.schoolUnit.address.province &&'disabled'}} name="unitProvince" class="text-input" type="text" required />
                    <input type="checkbox" id="noProvince" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.schoolUnit.address.province &&'checked="checked"'}}  name="noProvince" value="Y" class="align-middle">
                    <label for="noProvince" class="align-middle">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label class="field-title">邮编</label>
                    <input id="unitZipCode" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.address.postCode}}" name="unitZipCode" class="text-input" type="text" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.schoolUnit.address.postCode &&'disabled'}} required />
                    <input type="checkbox" id="noUnitZipCode" name="noUnitZipCode" value="Y" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.schoolUnit.address.postCode &&'checked="checked"'}} class="align-middle">
                    <label for="noUnitZipCode" class="align-middle">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label class="field-title">单位电话</label>
                    <input id="unitTel" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.schoolUnit.phoneNumber}}" name="unitTel" class="text-input phone" type="text" required />
                </div>
                <div class="field-container">
                    <span class="field-title">国家</span>
                    {{#countries | select:{id:"unitCountry",name:"unitCountry",defaultValue:form.wetInfo.presentWork?form.wetInfo.presentWork.schoolUnit.address.country:null,title:"请选择国家！",required:true} }}
                </div>
                <div class="field-container">
                    <label class="field-title">月薪</label>
                    <input id="income" value="{{form.wetInfo.presentWork && form.wetInfo.presentWork.income}}" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.income && 'disabled'}} name="income" class="text-input" type="text" required />
                    <input type="checkbox" id="noIncome" name="noIncome" value="Y" {{form.wetInfo.presentWork && !form.wetInfo.presentWork.income && 'checked="checked"'}} class="align-middle">
                    <label for="noIncome" class="align-middle">没有/无法提供</label>
                </div>
                <div class="field-container">
                    <label for="" class="field-title">请简要描述您的工作职责</label>
                    <textarea required name="duteDescription" id="duteDescription" cols="31" rows="3">{{form.wetInfo.presentWork && form.wetInfo.presentWork.duteDescription}}</textarea>
                </div>
            </div>
        </div>
        <div class="field-container">
            <span class="field-title">您之前是否有工作？</span>
            <input type="radio" id="haveWorkedYes" name="haveWorked" {{form.wetInfo.haveWorkBefore && 'checked="checked"'}} value="Y" data-tpl-id="prework-template" class="align-middle" />
            <label for="haveWorked" class="align-middle">是</label>
            <input type="radio" id="haveWorkedNo" name="haveWorked" {{!form.wetInfo.haveWorkBefore && 'checked="checked"'}} value="N"  class="align-middle" />
            <label for="haveWorked" class="align-middle">否</label>
            <div class="area info-area one {{!form.wetInfo.haveWorkBefore && 'hidden'}}">
                <span class="field-title">请在下方提供先前单位/工作信息</span>
                <hr/>
                <input type="hidden" id="workCount" counter name="workCount" value="{{ form.wetInfo.haveWorkBefore?form.wetInfo.previousWork.employmentList.length:0 }}" />
                <!--工作单位占位-->
                {{if form.wetInfo.haveWorkBefore}}
                    {{each form.wetInfo.previousWork.employmentList as item i}}
                    <div class="repeat">
                        <div class="field-container">
                            <label class="field-title">工作单位名称</label>
                            <input id="preUnitName_{{i}}" value="{{item.companyName}}" name="preUnitName_{{i}}" class="text-input english" type="text" required />
                        </div>
                        <div class="field-container">
                            <label class="field-title">单位地址</label>
                            <input id="preUnitStreet_{{i}}" value="{{item.address.street}}" name="preUnitStreet_{{i}}" class="address-input address" type="text" required />
                        </div>
                        <div class="field-container">
                            <label class="field-title">城市</label>
                            <input id="preUnitCity_{{i}}" value="{{item.address.city}}" name="preUnitCity_{{i}}" class="text-input" type="text" required />
                        </div>
                        <div class="field-container">
                            <label class="field-title">省份</label>
                            <input id="preUnitProvince_{{i}}" value="{{item.address.province}}"  {{!item.address.province && 'disabled'}} name="preUnitProvince_{{i}}" class="text-input" type="text" required />
                            <input type="checkbox" id="noPreProvince_{{i}}" name="noPreProvince_{{i}}" {{!item.address.province && 'checked="checked"'}} value="Y" class="align-middle"/>
                            <label for="noPreProvince" class="align-middle">没有/无法提供</label>
                        </div>
                        <div class="field-container">
                            <label class="field-title">邮编</label>
                            <input id="preUnitZipCode_{{i}}" value="{{item.address.postCode}}" {{!item.address.postCode && 'disabled'}} name="preUnitZipCode_{{i}}" class="text-input" type="text" required />
                            <input type="checkbox" id="noPreUnitZipCode_{{i}}" {{!item.address.postCode && 'checked="checked"'}} name="noPreUnitZipCode_{{i}}" value="Y" class="align-middle"/>
                            <label for="noPreUnitZipCode" class="align-middle">没有/无法提供</label>
                        </div>
                        <div class="field-container">
                            <label class="field-title">单位电话</label>
                            <input id="preUnitTel_{{i}}"  value="{{item.telephone}}" name="preUnitTel_{{i}}" class="text-input phone" type="text" required />
                        </div>
                        <div class="field-container">
                            <span class="field-title">国家</span> 
                            {{#countries | select:{id:"preUnitCountry_"+i,name:"preUnitCountry_"+i,defaultValue:item.address.country,title:"请选择国家！",required:true} }}
                        </div>

                        <div class="field-container">
                            <label class="field-title">职位</label>
                            <input id="jobTitle_{{i}}" value="{{item.jobTitle}}" name="jobTitle_{{i}}" class="text-input" type="text" required />
                        </div>
                        <div class="field-container">
                            <label class="field-title">上级领导的姓氏</label>
                            <input id="supervisorSurname_{{i}}" value="{{item.supervisor && item.supervisor.surnName}}" {{item.supervisor && !item.supervisor.surnName &&'disabled'}}  name="supervisorSurname_{{i}}" class="text-input" type="text" required />
                            <input type="checkbox" id="notKnowSupervisorSurname_{{i}}" {{item.supervisor && !item.supervisor.surnName &&'checked="checked"'}} name="notKnowSupervisorSurname_{{i}}" value="Y" class="align-middle"/>
                            <label for="notKnowSupervisorSurname" class="align-middle" >不知道</label>
                        </div>
                        <div class="field-container">
                            <label class="field-title">上级领导的名字</label>
                            <input id="supervisorGivenName_{{i}}" value="{{item.supervisor && item.supervisor.givenName}}" {{item.supervisor && !item.supervisor.givenName &&'disabled'}} name="supervisorGivenName_{{i}}" class="text-input" type="text" required />
                            <input type="checkbox" id="notKnowSupervisorGivenName_{{i}}" name="notKnowSupervisorGivenName_{{i}}" {{item.supervisor && !item.supervisor.givenName &&'checked="checked"'}} value="Y" class="align-middle"/>
                            <label for="notKnowSupervisorGivenName" class="align-middle">不知道</label>
                        </div>
                        <div class="field-container">
                            <label class="field-title">入职日期</label>
                            {{#item.from | renderDate:{name:"workFromDate_"+i,year:true,month:false,day:false} }}
                        </div>
                        <div class="field-container">
                            <label class="field-title">离职日期</label>
                            {{#item.to | renderDate:{name:"workToDate_"+i,year:true,month:false,day:false} }}                            
                        </div>
                        <div class="field-container">
                            <label for="" class="field-title">职责说明</label>
                            <textarea required  name="duteDescription_{{i}}" id="duteDescription_{{i}}" cols="31" rows="2">{{item.duteDescription}}</textarea>
                        </div>
                    </div>
                    {{/each}}
                {{/if}}
                <div id="operate_prework_menu" class="control-menu">
                    <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                    <a href="javascript:void(0)">删除</a>
                </div>
            </div>

        </div>
    </div>
    <hr/>
    <br/>
</script>

<script id="eduInfo-v-bind" type="text/html">
    <div id="eduInfo">
        <h2 class="area-title">教育经历</h2>
        <div class="field-container">
            <span class="field-title">您是否有中学及以上的教育经历？</span>
            <input type="radio" id="attendedYes" name="attended" value="Y" {{form.wetInfo.haveAttendSchool && 'checked="checked"'}}  data-tpl-id="edu-template" class="align-middle" />
            <label for="attendedYes" class="align-middle">是</label>
            <input type="radio" id="attendedNo" name="attended" value="N" {{!form.wetInfo.haveAttendSchool && 'checked="checked"'}}  class="align-middle" />
            <label for="attendedNo" class="align-middle">否</label>
            <div class="area info-area one {{!form.wetInfo.haveAttendSchool && 'hidden'}}">
                <span class="field-title">请提供以下有关你曾学习过的教育机构信息</span>
                <input type="hidden" id="eduCount" counter name="eduCount" value="{{form.wetInfo.haveAttendSchool?form.wetInfo.previousWork.institutionList.length:0}}" />
                <!--教育经历占位-->
                {{if form.wetInfo.haveAttendSchool}}
                    {{each form.wetInfo.previousWork.institutionList as item i}}
                        <div class="repeat">
                            <div class="field-container">
                                <label class="field-title">教育机构名称</label>
                                <input id="institutionName_{{i}}" value="{{item.name}}" name="institutionName_{{i}}" class="text-input english" type="text" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">教育机构地址</label>
                                <input id="institutionStreet_{{i}}" value="{{item.address.street}}" name="institutionStreet_{{i}}" class="address-input address" type="text" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">城市</label>
                                <input id="institutionCity_{{i}}" value="{{item.address.city}}" name="institutionCity_{{i}}" class="text-input" type="text" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">省份</label>
                                <input id="institutionProvince_{{i}}" value="{{item.address.province}}" {{!item.address.province && 'disabled'}}  name="institutionProvince_{{i}}" class="text-input" type="text" required />
                                <input type="checkbox" id="noInstitutionProvince_{{i}}" {{!item.address.province && 'checked="checked"'}} name="noInstitutionProvince" value="Y" class="align-middle">
                                <label for="noInstitutionProvince" class="align-middle">没有/无法提供</label>
                            </div>
                            <div class="field-container">
                                <label class="field-title">邮编</label>
                                <input id="institutionZipCode_{{i}}" value="{{item.address.postCode}}" {{!item.address.postCode && 'disabled'}} name="institutionZipCode_{{i}}" class="text-input" type="text" required />
                                <input type="checkbox" id="noInstitutionZipCode_{{i}}" {{!item.address.postCode && 'checked="checked"'}} name="noInstitutionZipCode_{{i}}" value="" class="align-middle">
                                <label for="noInstitutionZipCode" class="align-middle">没有/无法提供</label>
                            </div>
                            <div class="field-container">
                                <span class="field-title">国家</span> 
                                {{#countries | select:{id:"institutionCountry_"+i,name:"institutionCountry_"+i,defaultValue:item.address.country,title:"请选择国家！",required:true}
                                }}
                            </div>
                            <div class="field-container">
                                <label class="field-title">课程/专业名称</label>
                                <input id="course_{{i}}" value="{{item.course}}" name="course_{{i}}" class="text-input" type="text" required />
                            </div>
                            <div class="field-container">
                                <label class="field-title">就读开始日期</label>
                                {{#item.from | renderDate:{name:"attendFromDate_"+i,year:true,month:false,day:false} }}
                            </div>
                            <div class="field-container">
                                <label class="field-title">就读结束日期</label>
                                {{#item.to | renderDate:{name:"attendToDate_"+i,year:true,month:false,day:false} }}
                            </div>
                        </div>
                    {{/each}}
                {{/if}}
                <div id="operate_edu_menu" class="control-menu">
                    <a href="javascript:void(0)">添加</a>&nbsp;&nbsp;
                    <a href="javascript:void(0)">删除</a>
                </div>
            </div>
        </div>
    </div>
</script>
<script id="prework-template" type="text/html">
    <div class="repeat">
        <div class="field-container">
            <label class="field-title">工作单位名称</label>
            <input id="preUnitName" name="preUnitName" class="text-input english" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">单位地址</label>
            <input id="preUnitStreet" name="preUnitStreet" class="address-input address" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">城市</label>
            <input id="preUnitCity" name="preUnitCity" class="text-input" type="text" required />
        </div>
        <div class="field-container">
            <label class="field-title">省份</label>
            <input id="preUnitProvince" name="preUnitProvince" class="text-input" type="text" required />
            <input type="checkbox" id="noPreProvince" name="noPreProvince" value="Y" class="align-middle">
            <label for="noPreProvince" class="align-middle">没有/无法提供</label>
        </div>
        <div class="field-container">
            <label class="field-title">邮编</label>
            <input id="preUnitZipCode" name="preUnitZipCode" class="text-input" type="text" required />
            <input type="checkbox" id="noPreUnitZipCode" name="noPreUnitZipCode" value="Y" class="align-middle">
