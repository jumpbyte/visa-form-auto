body,p,li,ul,img,div,img{
	padding:0px;margin:0px;
}
.text{ padding-top:8%; margin-left:auto; margin-right:auto; height:70%; width:370px;}

.text p {  font-family:yahei microsoft; font-size:16px;  line-height:32px; color:#fff; font-weight:bold;}
.ziti {font-size:30px; font-family:KaiTi; color:#EE1289;}
.content{width: 100%; height:643px; background:url(../images/default/b-01.jpg) no-repeat;position:relative; background-size:100%;}
.content1{width: 100%; height:643px; background:url(../images/default/b-02.jpg) no-repeat; position:relative;background-size:100%;}

