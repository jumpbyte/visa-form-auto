.width1 { width:370px; }
.width2 { width:540px; }
.warn{ position:absolute; right:20px; height:20px; width:110px; top:10px; background-color:#000000;
    filter:Alpha(opacity=50);
    background: rgba(0,0,0,0.6);color:#FFF; font-size:12px; line-height:20px;  }
.close{ position:absolute; right:0px; height:15px; width:15px; top:0px;
    filter:Alpha(opacity=50);
    background: rgba(0,0,0,0.6);color:#FFF;}
.warn label{ vertical-align:top;}

.btn_close {
	padding: 2px 4px 3px 4px;
	margin: 0px;
	color: #333;
	background-color: #5B5B5B;
	border: 1px solid #DDD;
	cursor: pointer;
	min-height: 24px;
	-webkit-appearance: none;
	border-radius: 0;
	outline: none;
}
.chk_1 { margin: 4px 3px 3px 4px;}
