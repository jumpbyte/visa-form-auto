/*日期列表*/
.menu { padding:0; margin:0; list-style-type:none;}
.menu li { background:#e9f3f9; margin-right:1px; float:left; color:#3a3a3a;height:32px;}
.menu li a { display:block; width:80px; text-align:center; height:32px; line-height:32px; color:#3a3a3a; font-size:13px; text-decoration:none;}
.menu li a:hover {background: #2a84d0; color:#FFF}
.menu li a:visited {background: #2a84d0;}
a.xz{background:#2a84d0!important;color:#fff!important;height:32px;}
.cur{ background:#D96C00; font-weight:bold;}